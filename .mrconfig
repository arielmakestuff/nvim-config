[bundle/LanguageClient-neovim]
setup =
	   cd ..
	   git submodule update --init --recursive -- LanguageClient-neovim
	   cd LanguageClient-neovim
	   ./install.sh
add =
	  cd ..
	  git add LanguageClient-neovim

